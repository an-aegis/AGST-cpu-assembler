            ThrowPropertyIsReadOnlyError("OutputFile")
        End If
    End Sub
        
    Protected Sub PrepDC()
        hDC        ' ensures the hDC is prepped.  VB6 doesn't do this, but we need to as we currently store InternalCurrentX as a scaled pixel
    End Sub
    
