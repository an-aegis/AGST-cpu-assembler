    End Sub
    
    Public Sub PSet(ByVal Flags As Long, _
                        ByVal X As Single, _
                        ByVal Y As Single, _
                        ByVal Color As Long)
        If InternalPrintId = -1 Then StartPrint
        InternalIsBlankPage = False
